<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidcalenderproject.ethiocalendar">

    <application
        android:allowBackup="true"
        android:icon="@drawable/adeyic"
        android:label="@string/app_name"
        android:roundIcon="@drawable/adeyic"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ethiocalendar">
        <activity
            android:name=".splahActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Holiday"
            android:screenOrientation="portrait"/>


        <receiver android:name=".AlarmReciever"
            android:exported="true"
            android:enabled="true"></receiver>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>